{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "power-modules/console",
    "description": "PowerModuleSetup extension that auto-discovers and registers Symfony Console commands from Power Modules.",
    "type": "library",
    "keywords": [
        "modular",
        "framework",
        "power-modules",
        "dependency-injection",
        "symfony",
        "console",
        "cli"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Teterin",
            "email": "roundcubez@gmail.com",
            "role": "Owner"
        }
    ],
    "autoload": {
        "psr-4": {
            "Modular\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modular\\Console\\Test\\": "test/"
        }
    },
    "require": {
        "php": "^8.4",
        "power-modules/framework": "^2.1",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "friendsofphp/php-cs-fixer": "^3.88",
        "phpstan/phpstan": "^2.1"
    }
}